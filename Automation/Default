		SETGLOBAL speeding 0
			goto une
	end if
	if @my.right_item == "Moonstone"
		"/unequip moonstone\r"
	end if
end if
if logck >= hasfallen
	setglobal fallcall 1
	if isMorphed == 1
		SETGLOBAL isMorphed 0
			SETGLOBAL whatShape 0
	else if isMorphed == 0
		SETGLOBAL speeding 0
			goto une
	end if
end if
if logck >= "You slow down"
	if speeding == 1
		SETGLOBAL speeding 0
			goto une
	end if
end if
if Hchk == 1
	message "*Healer Mode"
		call sethealeritem
			SETGLOBAL Hchk 0
end if
if Rchk == 1
	message "*Ranger Mode"
		message "Type /ranger for commands"
			SETGLOBAL Rchk 0
				SETGLOBAL Rmess 1
end if
goto top
label une
	if swaobackL != "Heartwood Charm"
		if swaobackL != ""
			message "* swapping left hand back to" swaobackL " "
				"/equip " swaobackL " \r"
					SETGLOBAL swaobackL ""
			else 
			if @my.left_item == "Heartwood Charm"
				"/unequip heartwood \r"
			end if
	end if
end if
goto top
}
 
"/am"
{
if AT == 0
	setglobal AT 1
		message "*Auto Mate ON!"
else
	setglobal AT 0
		message "*Auto Mate OFF!"
end if
}
 
 
"/atz"
{
if trademe == 0
	setglobal trademe 1
		message "*Auto Trade Zero ON!"
			if AT == 0
				message "* Auto Mate is OFF, Auto Trade Zero will not work with it turned off."
	end if
else
	setglobal trademe 0
		message "*Auto Trade Zero OFF!"
end if
}

control-click
{
if CHAINING == 0
	setglobal swaoback @my.right_item
end if
	if @my.right_item != chain
			setglobal CHAINING 1
				"/equip " chain "\r"
					"/use " @click.simple_name " \r"
	else
	if @my.right_item == chain
			setglobal CHAINING 1
				"/use " @click.simple_name " \r"
	end if
end if
}

"/test"
{
if htest == 1
	message "* Healer Test Mode: ***OFF!***"
		SETGLOBAL htest 0
else if htest == 0
	message "* Healer Test Mode: ***ON!***"
		SETGLOBAL htest 1
end if
}

control-F1
{
if CHAINING == 1
	setglobal CHAINING 0 
		"/equip " swaoback "\r"
end if
}

control-F2
{
if conc2 == 0
	setglobal conc2 1
		message "control-click2: chain mode"
else if conc2 == 1
	setglobal conc2 0
		message "control-click2: sell mode"
end if
}

shift-click
{
if shiftc2 == 0
	setglobal shiftc2 1
		message "shift-click2: pull mode"
else if shiftc2 == 1
	setglobal shiftc2 2
		message "shift-click2: push mode"
else if shiftc2 == 2
	setglobal shiftc2 0
		message "shift-click2: share mode"
end if
}

control-click2
{
if conc2 == 0
	"/sell 0 " @clicksplayer " \r"
else if conc2 == 1
	if CHAINING == 0
		setglobal swaoback @my.right_item
	end if
	if @my.right_item != chain
		setglobal CHAINING 1
			"/equip " chain "\r"
				"/use " @click.simple_name " \r"
	end if
	else
	if @my.right_item == chain
		setglobal CHAINING 1
			"/use " @click.simple_name " \r"
	end if
end if
}

shift-click2
{
$any_click
if shiftc2 == 0
message ping1
	"/share " @clicksplayer " \r"
else if shiftc2 == 1
message ping2
	if @click.name != ""
		"/whisper excuse me " @click.name " \r"
			"/pull " @click.simple_name " \r"
	else
		"/pull /critter \r"
	end if
else if shiftc2 == 2
message ping3
	if @click.name != ""
		"/whisper excuse me " @click.name " \r"
			"/push " @click.simple_name " \r"
	end if
end if
}



//not finshed.

"/if"
{
set fn 1
label gofind
set logck @env.textlog
"/selectitem " find[fn] "\r"
pause 3
if logck != "No item"
	setglobal item[nls] @my.selected_item
		if @my.selected_item >= "<#"
			call findagain
		end if
		if nls < 33
			setglobal nls + 1
		else
			goto end
		end if
end if
if fn < 27
	set fn + 1
else
	goto end
end if
goto gofind
label end
}

finagain
{
set num 2
label anotherone
"selectitem " item[nls] "<#" num "\r"
	pause 3
		if logck != "No item"
			goto exit
		else
			if nls < 33
				setglobal nls + 1
			else
				goto exit
			end if
				setglobal item[nls] @my.selected_item
					goto anotherone
						set num + 1
		end if
label exit
}

setglobal nls 1
setgloabal find[1] a
setgloabal find[2] b
setgloabal find[3] c
setgloabal find[4] d
setgloabal find[5] e

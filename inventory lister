
//not finshed.

"/if"
{
set fndr 1
label gofind
set logck @env.textlog
"/selectitem " find[fndr] "\r"
pause 3
if logck != "No item"
	setglobal item[nls] @my.selected_item
		if @my.selected_item >= "<#"
			set matchnum nls
				call findagain
					if matchnum == nls
						if nls < 32
							setglobal nls + 1
						else
							goto end
						end if
							goto gonext
						end if
					end if
		else if @my.selected_item >= "("
			set matchnum nls
				call findagain
					if matchnum == nls
						if nls < 32
							setglobal nls + 1
						else
							goto end
						end if
							goto gonext
						end if
					end if
		end if
end if
label gonext
if fndr < 26
	set fndr + 1
else
	call printlist
end if
goto gofind
label end
}

findagain
{
set num 2
label anotherone
"/selectitem " item[nls] "<#" num "\r"
	pause 3
		if logck != "No item"
			goto exit
		else
			if nls < 33
				setglobal nls + 1
			else
				goto exit
			end if
				setglobal item[nls] @my.selected_item
					set num + 1
						goto anotherone
		end if
label exit
}

printlist
{
set numby 1
message "* Inventory List:"
label top
if numby < 32
	if item[numby] != "Nothing"
		if item[numby] != ""
			message "" item[numby] ""
				set numby + 1
		else
			set numby + 1
				goto top
		end if
	else
		set numby + 1
			goto top
	end if
goto top
else if numby > 32
		goto end
end if
label end
	message "* ------end------ *"
}

setglobal nls 1
setglobal find[1] a
